<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Visit.
    -->
    <changeSet id="20201128080221-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="patient_id_id"
                                 baseTableName="visit"
                                 constraintName="fk_visit_patient_id_id"
                                 referencedColumnNames="id"
                                 referencedTableName="person"/>

        <addForeignKeyConstraint baseColumnNames="doctor_id_id"
                                 baseTableName="visit"
                                 constraintName="fk_visit_doctor_id_id"
                                 referencedColumnNames="id"
                                 referencedTableName="person"/>

    </changeSet>
</databaseChangeLog>
